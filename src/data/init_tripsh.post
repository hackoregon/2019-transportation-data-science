-- parse text timestamp
ALTER TABLE ENDDATE_init_tripsh
  ADD COLUMN opd_date timestamp;
UPDATE ENDDATE_init_tripsh
  SET opd_date = to_timestamp(opd_date_text, 'DDMonYYYY:HH24:MI:SS');

-- create a primary key
ALTER TABLE ENDDATE_init_tripsh
    ADD COLUMN id serial;
ALTER TABLE ENDDATE_init_tripsh
    ADD CONSTRAINT ENDDATE_init_tripsh_pkey PRIMARY KEY (id);
